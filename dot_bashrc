# If not running interactively, don't do anything
[[ $- != *i* ]] && return

alias ls='ls --color=auto'
alias grep='grep --color=auto'
PS1='\[\e[92m\][\[\e[95m\]\t\[\e[0m\] \[\e[92m\]-\[\e[0m\] \u\[\e[92m\]@\[\e[0m\]\H \[\e[94;7m\]\W\[\e[0;92m\]]\[\e[0m\] \[\e[92m\]\\$\[\e[0m\] '

# Exports
export EDITOR=/usr/bin/nano
[[ -s "/home/dante/.sdkman/bin/sdkman-init.sh" ]] && source "/home/dante/.sdkman/bin/sdkman-init.sh"

fortune
